{
	"name": "coblocks/gallery-carousel",
	"category": "media",
	"attributes": {
		"carouselId" : {
			"type": "string"
		},
		"gutter": {
			"type": "number",
			"default": 5
		},
		"gutterMobile": {
			"type": "number",
			"default": 5
		},
		"height": {
			"type": "number",
			"default": 400
		},
		"prevNextButtons": {
			"type": "boolean",
			"default": true
		},
		"autoPlay": {
			"type": "boolean",
			"default": false
		},
		"autoPlaySpeed": {
			"type": "string",
			"default": 3000
		},
		"thumbnails": {
			"type": "boolean",
			"default": false
		},
		"responsiveHeight": {
			"type": "boolean",
			"default": false
		}
	}
}
